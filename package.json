{
  "name": "movie-rating-system",
  "version": "1.0.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "node scripts/load-config.js && next dev",
    "build": "node scripts/load-config.js && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "node scripts/load-config.js && prisma db push",
    "db:studio": "node scripts/load-config.js && prisma studio",
    "db:seed": "node scripts/load-config.js && tsx prisma/seed.ts",
    "db:clear": "node scripts/load-config.js && tsx prisma/clear-data.ts",
    "db:reset": "node scripts/load-config.js && tsx prisma/clear-data.ts && node scripts/load-config.js && tsx prisma/seed.ts",
    "db:generate": "node scripts/load-config.js && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
